{
  "name": "edutube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push && next build",
    "build:dev": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "build:netlify": "npm run db:generate && npm run build",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma migrate reset",
    "db:remove-shorts": "npx tsx scripts/remove-shorts.ts",
    "setup:chatgpt-oss": "./scripts/setup-chatgpt-oss.sh",
    "test:youtube-api": "node scripts/test-youtube-api.js",
    "monitor:health": "curl http://localhost:3000/api/health | jq",
    "monitor:quota": "curl http://localhost:3000/api/youtube/quota | jq",
    "monitor:quota-multikey": "curl http://localhost:3000/api/youtube/quota-multikey | jq",
    "cache:clear": "curl -X DELETE http://localhost:3000/api/youtube/quota",
    "cache:cleanup": "curl -X DELETE http://localhost:3000/api/health",
    "apikeys:check": "node scripts/manage-api-keys.js check",
    "apikeys:test": "node scripts/manage-api-keys.js test",
    "apikeys:quota": "node scripts/manage-api-keys.js quota",
    "apikeys:reset": "node scripts/manage-api-keys.js reset",
    "apikeys:monitor": "node scripts/manage-api-keys.js monitor",
    "apikeys:recommend": "node scripts/manage-api-keys.js recommend",
    "apikeys:all": "node scripts/manage-api-keys.js all",
    "db:test": "node scripts/test-database.js",
    "db:test-with-sample": "node scripts/test-database.js --create-sample",
    "db:seed-production": "node scripts/test-database.js seed"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.14",
    "bcryptjs": "^3.0.2",
    "googleapis": "^156.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.21",
    "prisma": "^6.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "eslint-config-next": "^15.4.6",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  }
}
