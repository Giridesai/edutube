# =============================================================================
# API RATE LIMITING & LOAD BALANCING CONFIGURATION
# =============================================================================
# Enable distributed load balancing across API keys
ENABLE_API_LOAD_BALANCING=true

# Maximum requests per minute per API key
MAX_REQUESTS_PER_MINUTE_PER_KEY=100

# Maximum daily quota per API key
MAX_DAILY_QUOTA_PER_KEY=10000

# Failover settings
ENABLE_API_FAILOVER=true
API_FAILURE_RETRY_COUNT=3
API_FAILURE_COOLDOWN_MINUTES=5

# =============================================================================
# MONITORING & LOGGING
# =============================================================================
# Enable API usage monitoring
ENABLE_API_MONITORING=true

# Log API key usage rotation
LOG_API_KEY_ROTATION=true

# Monitor quota usage across all keys
MONITOR_QUOTA_USAGE=true

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================
# Development mode settings
NODE_ENV="development"

# Enable debug logging
DEBUG_API_CALLS=false
DEBUG_QUOTA_USAGE=false
DEBUG_RATE_LIMITING=false

# Test mode (uses lower quotas for testing)
TEST_MODE=false
TEST_MODE_QUOTA_LIMIT=100

# =============================================================================
# CACHE CONFIGURATION
# =============================================================================
# Cache settings for better performance
ENABLE_REDIS_CACHE=false
REDIS_URL=""

# Memory cache settings
MEMORY_CACHE_TTL_SECONDS=300
DATABASE_CACHE_TTL_SECONDS=1800

# =============================================================================
# BACKUP & RECOVERY
# =============================================================================
# Database backup settings
AUTO_BACKUP_ENABLED=false
BACKUP_INTERVAL_HOURS=24
BACKUP_RETENTION_DAYS=7

